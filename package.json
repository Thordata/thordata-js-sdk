{
  "type": "module",
  "name": "thordata-js-sdk",
  "version": "0.1.0",
  "description": "Official JavaScript/TypeScript SDK for Thordata (SERP API, Web Unlocker, Web Scraper API).",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "pretest": "npm run build",
    "test": "vitest run",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\" \"examples/**/*.ts\"",
    "format": "prettier -w .",
    "format:check": "prettier -c ."
  },
  "keywords": [
    "thordata",
    "serp",
    "web-scraping",
    "proxy",
    "web-unlocker",
    "typescript",
    "sdk"
  ],
  "author": "Thordata Developer Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.0",
    "dotenv": "^16.4.5",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.8.1",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.39.2",
    "nock": "^14.0.10",
    "prettier": "^3.7.4",
    "ruff": "^1.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vitest": "^4.0.16"
  },
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Thordata/thordata-js-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/Thordata/thordata-js-sdk/issues"
  },
  "homepage": "https://github.com/Thordata/thordata-js-sdk#readme"
}
