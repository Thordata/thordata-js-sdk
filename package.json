{
  "type": "module",
  "name": "@thordata/js-sdk",
  "version": "2.0.0",
  "description": "Official Node.js/TypeScript SDK for Thordata (SERP, Web Unlocker, Web Scraper Tasks, Public API).",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "pretest": "npm run build",
    "test": "vitest run",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\" \"examples/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" \"tests/**/*.ts\" \"examples/**/*.ts\" --fix",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "format:fix": "prettier --write .",
    "prepack": "npm run build",
    "prepublishOnly": "npm run format:check && npm run lint && npm test",
    "example:verify": "npm run build && node dist/examples/verify_new_features.js",
    "example:serp": "tsx examples/serp_basic.ts",
    "example:universal": "tsx examples/universal_basic.ts",
    "example:task": "tsx examples/basic_scraper_task.ts",
    "example:proxy:residential": "tsx examples/proxy_residential.ts",
    "example:proxy:mobile": "tsx examples/proxy_mobile.ts",
    "example:proxy:datacenter": "tsx examples/proxy_datacenter.ts",
    "example:proxy:isp": "tsx examples/proxy_isp.ts"
  },
  "keywords": [
    "thordata",
    "serp",
    "web-scraping",
    "proxy",
    "web-unlocker",
    "typescript",
    "sdk"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Thordata Developer Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.0",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.6",
    "socks": "^2.8.3",
    "socks-proxy-agent": "^8.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.8.1",
    "@vitest/coverage-v8": "^4.0.16",
    "dotenv": "^16.6.1",
    "eslint": "^9.39.2",
    "nock": "^14.0.10",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vitest": "^4.0.16"
  },
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Thordata/thordata-js-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/Thordata/thordata-js-sdk/issues"
  },
  "homepage": "https://github.com/Thordata/thordata-js-sdk#readme"
}
