# Proxy examples
THORDATA_PROXY_USERNAME=your_proxy_username_here
THORDATA_PROXY_PASSWORD=your_proxy_password_here
THORDATA_PROXY_HOST=t.pr.thordata.net
THORDATA_PROXY_PORT=9999